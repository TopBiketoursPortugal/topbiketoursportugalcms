{
  "name": "astro-bookshop-starter",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "//install": "pnpm exec husky init && echo pnpm lint-staged > .husky/pre-commit",
    "dev": "astro dev",
    "start": "pnpm build && static-web-server -t true --root ./dist --http2-tls-cert ./.certs/local.pem  --http2-tls-key ./.certs/local.key -g info",
    "start:prod": "pnpm build:prod && static-web-server -t true --root ./dist --http2-tls-cert ./.certs/cert.pem  --http2-tls-key ./.certs/key.pem -g info",
    "build": "astro build",
    "build:prod": "astro build && pnpm dlx @divriots/jampack ./dist && pnpm dlx html-minifier --input-dir ./dist --output-dir ./dist --file-ext html --collapse-whitespace --minify-js",
    "preview": "astro preview",
    "astro": "astro",
    "codegen": "astro sync && astro check",
    "lint": "pnpm eslint **/*.*",
    "check:build": "lychee --offline ./dist",
    "generate:types": "pnpm json2ts platforms.schema.json > platforms.d.ts",
    "generate": "pnpm dlx @bookshop/generate",
    "json2ts": "json2ts",
    "//prepare": "husky"
  },
  "dependencies": {
    "@astrojs/alpinejs": "0.4.3",
    "@astrojs/mdx": "4.2.0",
    "@astrojs/react": "4.2.1",
    "@astrojs/tailwind": "6.0.0",
    "@astrolib/analytics": "0.6.1",
    "@bookshop/astro-bookshop": "3.14.0",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-regular-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@iconify-json/circle-flags": "1.2.6",
    "@iconify-json/fa": "1.2.1",
    "@iconify-json/logos": "1.2.4",
    "@iconify-json/ph": "1.2.2",
    "@phosphor-icons/core": "^2.1.1",
    "@phosphor-icons/react": "2.1.7",
    "@playform/compress": "^0.1.7",
    "@types/alpinejs": "3.13.11",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "alpinejs": "3.14.9",
    "astro": "5.5.2",
    "astro-breadcrumbs": "^3.3.1",
    "astro-ui-avatars": "^0.0.3",
    "clsx": "2.1.1",
    "nightwind": "^1.1.13",
    "punycode": "2.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remove-markdown": "0.6.0",
    "sharp": "0.33.5",
    "tailwind-merge": "3.0.2",
    "tailwind-variants": "1.0.0",
    "tailwindcss": "3.3.3",
    "uuid": "11.1.0",
    "vanilla-cookieconsent": "3.1.0"
  },
  "devDependencies": {
    "@astrojs/rss": "4.0.11",
    "@astrojs/sitemap": "3.2.1",
    "@bookshop/astro-engine": "3.14.0",
    "@bookshop/browser": "3.14.0",
    "@bookshop/generate": "3.14.0",
    "@iconify/react": "5.2.0",
    "@kindspells/astro-shield": "^1.7.1",
    "@playform/inline": "0.1.1",
    "@sentry/astro": "^9.5.0",
    "@spotlightjs/astro": "^3.1.0",
    "@tailwindcss/typography": "0.5.16",
    "@types/markdown-it": "14.1.2",
    "@vite-pwa/astro": "0.5.0",
    "astro-favicons": "3.1.5",
    "astro-icon": "1.1.5",
    "astro-meta-tags": "^0.3.1",
    "astro-seo": "0.8.4",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "json-schema-to-typescript": "15.0.4",
    "markdown-it": "14.1.0",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-astro-organize-imports": "0.4.11",
    "prettier-plugin-tailwindcss": "0.6.11",
    "slugify": "1.6.6",
    "svg-sprite": "^2.0.4",
    "svgo": "^3.3.2",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "1.0.7",
    "tailwindcss-mixins": "^0.0.11",
    "tailwindcss-multi": "^0.4.6",
    "tailwindcss-selector-patterns": "0.0.16",
    "tailwindcss-signals": "0.0.25",
    "type-fest": "4.37.0"
  },
  "packageManager": "pnpm@10.6.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@sentry/cli",
      "esbuild",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "@sentry/cli"
    ]
  }
}
